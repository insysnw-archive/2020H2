+vec3 top_np = top.rgb / top.a;
+float sqrtf = sqrt(bot_np.r);
+float dr = dofx(bot_np.r, sqrtf);
+sqrtf = sqrt(bot_np.g);
+float dg = dofx(bot_np.g, sqrtf);
+sqrtf = sqrt(bot_np.b);
+float db = dofx(bot_np.b, sqrtf);
