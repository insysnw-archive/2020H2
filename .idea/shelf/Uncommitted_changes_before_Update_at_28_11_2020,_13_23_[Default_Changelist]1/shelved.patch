+res.b = top.b;
+}
+ else if (top.a == 0.0){
+res.b = bot.b;
+}
+ else if (top_np.b <= 0.5){
+res.b = bot.b + (1.0 - bot.a) * top.b - top.a * bot.b * (1.0 - 2.0 * top_np.b) * (1.0 - bot_np.b);
+}
