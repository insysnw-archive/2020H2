+uniform vec4 xParams;
+uniform vec4 yParams;
+uniform vec3 perspVec;
+uniform vec4 content;
+lowp vec4 paint(vec2 winCoord) {
+vec3 fragCoord = vec3(winCoord.x, winCoord.y, 1.0);
