+vec2 pixcoord = vec2(
+    gl_FragCoord.x-jsl_pixCoordOffset.x,
+    ((jsl_pixCoordOffset.z-gl_FragCoord.y)*jsl_pixCoordOffset.w)-jsl_pixCoordOffset.y);
+const int MAX_FRACTIONS = 12;
